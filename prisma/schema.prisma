// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Test {
  id String @id @default(cuid())
}

model BlogPost {
  id          String @id @default(cuid())
  title       String
  slug        String @unique @default("test")
  time        String
  description String
  content     String
  category    String
}
